{
  "description": "automates redundant form submissions",
  "manifest_version": 2,
  "name": "Form Remember",
  "version": "1.0.0",
  "homepage_url": "https://github.com/jakec888/form-remember",

  "icons": {
    "48": "icons/favicon-48x48.png"
  },

  "permissions": ["activeTab"],

  "browser_action": {
    "default_icon": "icons/favicon-32x32.png",
    "theme_icons": [
      {
        "light": "icons/favicon-32x32.png",
        "dark": "icons/favicon-32x32.png",
        "size": 32
      }
    ],
    "default_title": "Form Reminder",
    "default_popup": "popup/index.html"
  }
}
